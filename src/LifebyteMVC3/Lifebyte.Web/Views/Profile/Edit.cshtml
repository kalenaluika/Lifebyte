@model Lifebyte.Web.Models.Core.Entities.Volunteer
@{
    ViewBag.Title = "Lifebyte - Edit Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>
    Edit Profile</h2>
@using (Html.BeginForm("Edit", "Profile", FormMethod.Post))
{
    @Html.ValidationSummary()
    @Html.EditorForModel()
    <p>
        <input type="submit" value="Update Profile" /></p>
}
<script type="text/javascript">
    choose("State_StateDropdownList", "State", "@Model.State");
</script>